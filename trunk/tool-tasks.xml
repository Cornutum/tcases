<project name="toolTasks" default="none" basedir=".">

  <property name="tools.build.lib" location="${root.dir}/tools/build/lib"/>

  <path id="tools.build.classpath">
    <fileset dir="${tools.build.lib}">
      <include name="*.jar"/>
    </fileset>
  </path>

  <!-- Utility tasks: if, etc. -->
