{
    "openapi": "3.0.0",
    "info": {
        "title": "Parameters",
        "version": "0.0.0"
    },
    "paths": {
        "/parameters/multiple": {
            "get": {
                "parameters": [
                    {
                        "name": "objectParam",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "value": {
                                    "type": "number"
                                }
                            }
                        }
                    },
                    {
                        "name": "objectParam",
                        "in": "cookie",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "tag": {
                                    "type": "string"
                                },
                                "id": {
                                    "type": "integer",
                                    "minimum": 0
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/success"
                    },
                    "default": {
                        "$ref": "#/components/responses/failure"
                    }
                }
            }
        }
    },
    "components": {
        "responses": {
            "success": {
                "description": "Success",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object"
                        }
                    }
                }
            },
            "failure": {
                "description": "Error",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object"
                        }
                    }
                }
            }
        }
    }
}
