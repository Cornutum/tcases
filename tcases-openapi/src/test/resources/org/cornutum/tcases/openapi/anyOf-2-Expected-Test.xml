<?xml version="1.0"?>
<TestCases system="AnyOf">
  <Has name="server" value="/"/>
  <Has name="version" value="0.0.0"/>
  <Function name="POST_anyOf">
    <Has name="operation" value="POST"/>
    <Has name="path" value="/anyOf"/>
    <Has name="server" value="/"/>
    <Has name="version" value="0.0.0"/>
    <TestCase id="0">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="application-json,application-jsonValue,Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="application-json">
          <Has name="mediaType" value="application/json"/>
        </Var>
        <Var name="Body.application-json.Type" value="number"/>
        <Var name="Body.application-json.Value.Is" value="0.0"/>
      </Input>
    </TestCase>
    <TestCase id="1">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="application-json,Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="application-json">
          <Has name="mediaType" value="application/json"/>
        </Var>
        <Var name="Body.application-json.Type" value="null"/>
        <Var name="Body.application-json.Value.Is" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="2">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="application-json,application-jsonValue,Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="application-json">
          <Has name="mediaType" value="application/json"/>
        </Var>
        <Var name="Body.application-json.Type" value="number"/>
        <Var name="Body.application-json.Value.Is" value="100.0"/>
      </Input>
    </TestCase>
    <TestCase id="3" failure="true">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="No" failure="true"/>
        <Var name="Body.Media-Type" NA="true"/>
        <Var name="Body.application-json.Type" NA="true"/>
        <Var name="Body.application-json.Value.Is" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="4" failure="true">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="Other" failure="true">
          <Has name="excluded" value="&apos;application/json&apos;"/>
        </Var>
        <Var name="Body.application-json.Type" NA="true"/>
        <Var name="Body.application-json.Value.Is" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="5" failure="true">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="application-json,Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="application-json">
          <Has name="mediaType" value="application/json"/>
        </Var>
        <Var name="Body.application-json.Type" value="Not number" failure="true"/>
        <Var name="Body.application-json.Value.Is" NA="true"/>
      </Input>
    </TestCase>
    <TestCase id="6" failure="true">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="application-json,application-jsonValue,Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="application-json">
          <Has name="mediaType" value="application/json"/>
        </Var>
        <Var name="Body.application-json.Type" value="number"/>
        <Var name="Body.application-json.Value.Is" value="-0.1" failure="true"/>
      </Input>
    </TestCase>
    <TestCase id="7" failure="true">
      <Has name="operation" value="POST"/>
      <Has name="path" value="/anyOf"/>
      <Has name="properties" value="application-json,application-jsonValue,Content"/>
      <Has name="server" value="/"/>
      <Has name="version" value="0.0.0"/>
      <Input type="request">
        <Var name="Body.Defined" value="Yes"/>
        <Var name="Body.Media-Type" value="application-json">
          <Has name="mediaType" value="application/json"/>
        </Var>
        <Var name="Body.application-json.Type" value="number"/>
        <Var name="Body.application-json.Value.Is" value="100.1" failure="true"/>
      </Input>
    </TestCase>
  </Function>
</TestCases>
