<?xml version="1.0"?>
<System name="realize">
  <Has name="server" value="/"/>
  <Has name="title" value="realize"/>
  <Has name="version" value="0.0.0"/>
  <Function name="POST_realize-param">
    <Has name="operation" value="POST"/>
    <Has name="path" value="/realize/{param}"/>
    <Input type="path">
      <VarSet name="param">
        <Has name="paramName" value="param"/>
        <Var name="Defined">
          <Has name="style" value="simple"/>
          <Value name="Yes" property="param"/>
          <Value name="No" failure="true"/>
        </Var>
        <Var name="Type" when="param">
          <Value name="string" property="paramValue"/>
          <Value name="null" failure="true"/>
          <Value name="Not string" failure="true"/>
        </Var>
        <VarSet name="Value" when="paramValue">
          <Var name="Length">
            <Value name="0" failure="true"/>
            <Value name="1" property="paramLengthValid, paramNotEmpty"/>
            <Value name="&gt; 1" property="paramLengthValid, paramNotEmpty"/>
          </Var>
        </VarSet>
      </VarSet>
    </Input>
    <Input type="request">
      <VarSet name="Body">
        <Var name="Defined">
          <Value name="Yes" property="Content"/>
          <Value name="No" failure="true"/>
        </Var>
        <Var name="Media-Type" when="Content">
          <Value name="application-json" property="application-json">
            <Has name="mediaType" value="application/json"/>
          </Value>
          <Value name="Other" failure="true">
            <Has name="excluded" value="&apos;application/json&apos;"/>
          </Value>
        </Var>
        <VarSet name="application-json" when="application-json">
          <Var name="Type">
            <Value name="object" property="application-jsonValue"/>
            <Value name="null" failure="true"/>
            <Value name="Not object" failure="true"/>
          </Var>
          <VarSet name="Value" when="application-jsonValue">
            <VarSet name="Properties">
              <VarSet name="id">
                <Var name="Defined">
                  <Has name="propertyName" value="id"/>
                  <Value name="Yes" property="application-jsonId, application-jsonProperties"/>
                  <Value name="No" failure="true"/>
                </Var>
                <Var name="Type" when="application-jsonId">
                  <Value name="integer" property="application-jsonIdValue"/>
                  <Value name="null" failure="true"/>
                  <Value name="Not integer" failure="true"/>
                </Var>
                <VarSet name="Value" when="application-jsonIdValue">
                  <Var name="Is">
                    <Value name="-1" failure="true"/>
                    <Value name="0"/>
                    <Value name="&gt; 0"/>
                  </Var>
                </VarSet>
              </VarSet>
              <VarSet name="tag">
                <Var name="Defined">
                  <Has name="propertyName" value="tag"/>
                  <Value name="Yes" property="application-jsonProperties, application-jsonTag"/>
                  <Value name="No" failure="true"/>
                </Var>
                <Var name="Type" when="application-jsonTag">
                  <Value name="string" property="application-jsonTagValue"/>
                  <Value name="null" failure="true"/>
                  <Value name="Not string" failure="true"/>
                </Var>
                <VarSet name="Value" when="application-jsonTagValue">
                  <Var name="Length">
                    <Value name="0" failure="true"/>
                    <Value name="1" property="application-jsonTagLengthValid, application-jsonTagNotEmpty"/>
                    <Value name="&gt; 1" property="application-jsonTagLengthValid, application-jsonTagNotEmpty"/>
                  </Var>
                </VarSet>
              </VarSet>
              <Var name="Additional">
                <Value name="Yes" failure="true"/>
                <Value name="No"/>
              </Var>
            </VarSet>
          </VarSet>
        </VarSet>
      </VarSet>
    </Input>
  </Function>
</System>
