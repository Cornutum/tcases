<?xml version="1.0"?>
<System name="realize">
  <Has name="server" value="/"/>
  <Has name="title" value="realize"/>
  <Has name="version" value="0.0.0"/>
  <Function name="POST_realize">
    <Has name="operation" value="POST"/>
    <Has name="path" value="/realize"/>
    <Input type="query">
      <VarSet name="param">
        <Has name="paramName" value="param"/>
        <Var name="Defined">
          <Has name="style" value="form"/>
          <Value name="Yes" property="param"/>
          <Value name="No" failure="true"/>
        </Var>
        <Var name="Type" when="param">
          <Value name="integer" property="paramValue"/>
          <Value name="null" failure="true"/>
          <Value name="Not integer" failure="true"/>
        </Var>
        <VarSet name="Value" when="paramValue">
          <Var name="Is">
            <Value name="&lt; 0"/>
            <Value name="0"/>
            <Value name="&gt; 0"/>
          </Var>
        </VarSet>
      </VarSet>
    </Input>
    <Input type="request">
      <VarSet name="Body">
        <Var name="Defined">
          <Value name="Yes" property="Content"/>
          <Value name="No" failure="true"/>
        </Var>
        <Var name="Media-Type" when="Content">
          <Value name="application-x-www-form-urlencoded" property="application-x-www-form-urlencoded">
            <Has name="mediaType" value="application/x-www-form-urlencoded"/>
          </Value>
          <Value name="Other" failure="true">
            <Has name="excluded" value="&apos;application/x-www-form-urlencoded&apos;"/>
          </Value>
        </Var>
        <VarSet name="application-x-www-form-urlencoded" when="application-x-www-form-urlencoded">
          <Var name="Type">
            <Value name="string" property="application-x-www-form-urlencodedValue"/>
            <Value name="null" once="true"/>
            <Value name="Not string" failure="true"/>
          </Var>
          <VarSet name="Value" when="application-x-www-form-urlencodedValue">
            <Var name="Length">
              <Value name="0" failure="true"/>
              <Value name="1" property="application-x-www-form-urlencodedLengthValid, application-x-www-form-urlencodedNotEmpty"/>
              <Value name="&gt; 1" property="application-x-www-form-urlencodedLengthValid, application-x-www-form-urlencodedNotEmpty"/>
            </Var>
          </VarSet>
        </VarSet>
      </VarSet>
    </Input>
  </Function>
</System>
