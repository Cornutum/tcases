              </Var>
            </VarSet>
            <Var name="Additional">
              <Value name="Yes" property="param0Properties"/>
              <Value name="No" failure="true"/>
            </Var>
          </VarSet>
        </VarSet>
      </VarSet>
    </Input>
  </Function>
