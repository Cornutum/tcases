                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "integer",
                                "minimum": 0
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/success"
                    },
                    "default": {
                        "$ref": "#/components/responses/failure"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "apiKeyCookie": {
                "type": "apiKey",
                "in": "cookie",
                "name": "apiKey"
            },
            "apiKeyHeader": {
                "type": "apiKey",
